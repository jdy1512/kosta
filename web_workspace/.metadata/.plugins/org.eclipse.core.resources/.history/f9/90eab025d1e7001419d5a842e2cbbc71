<script type="text/javascript">
	$(document).ready(function() {
		$("#btn").on("click", function() {
			$.getJSON("/jQuery_class/jQueryCtr", {"command":"getMemberByIdJson", "id":$("#id")}, function(obj) {
				var txt = obj.id + "<br/>" + obj.name + "<br/>" + obj.age;
				$("#first").html(txt);
			});
		});
	});
</script>

